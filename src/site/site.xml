<project name="${project.name}" xmlns="https://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd">
    <bannerLeft>
        <src>https://tomcat.apache.org/images/tomcat.png</src>
        <href>https://tomcat.apache.org/</href>
    </bannerLeft>
    <bannerRight>
        <src>https://logback.qos.ch/images/logos/lblogo.jpg</src>
        <href>https://logback.qos.ch/</href>
    </bannerRight>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.11.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>
    <body>
        <links>
            <item name="Tomcat Slf4j Logback" href="https://github.com/tomcat-slf4j-logback/tomcat7-slf4j-logback" />
